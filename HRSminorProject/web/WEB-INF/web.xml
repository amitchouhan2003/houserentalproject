<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>MyLoginServlet</servlet-name>
        <servlet-class>LoginAregistration.MyLoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MyRegistrationServlet</servlet-name>
        <servlet-class>LoginAregistration.MyRegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchHouse</servlet-name>
        <servlet-class>Searchanddisplay.RegisterHouse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterHouse</servlet-name>
        <servlet-class>Searchanddisplay.RegisterHouse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Searchhouse</servlet-name>
        <servlet-class>Searchanddisplay.Searchhouse</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyLoginServlet</servlet-name>
        <url-pattern>/MyLoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MyRegistrationServlet</servlet-name>
        <url-pattern>/MyRegistrationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchHouse</servlet-name>
        <url-pattern>/SearchHouse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterHouse</servlet-name>
        <url-pattern>/RegisterHouse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Searchhouse</servlet-name>
        <url-pattern>/Searchhouse</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
